{
  "name": "eslint-config-webstronauts",
  "version": "1.0.0",
  "description": "Standard eslint rules for all Webstronauts JavaScript projects",
  "license": "MIT",
  "repository": "webstronauts/eslint-config-webstronauts",
  "author": "Robin van der Vleuten <robin@webstronauts.co> (https://www.robinvdvleuten.nl)",
  "main": "dist/index.js",
  "jsnext:main": "src/index.js",
  "keywords": [
    "eslint",
    "eslint-config"
  ],
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "build": "rm -rf dist && rollup -c rollup.config.js -f cjs -o $npm_package_main $npm_package_jsnext_main",
    "lint": "eslint src",
    "test": "npm run lint && npm run build",
    "prepublish": "npm test"
  },
  "eslintConfig": {
    "extends": "./dist/index.js"
  },
  "dependencies": {
    "babel-eslint": "^8.0.1",
    "eslint-config-airbnb": "^16.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0"
  },
  "devDependencies": {
    "eslint": "^4.8.0",
    "rollup": "^0.50.0",
    "rollup-plugin-buble": "^0.16.0"
  },
  "peerDependencies": {
    "eslint": "^4.8.0"
  }
}
